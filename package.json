  "author": "brockfanning <brockfanning@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/usdoj/exitscript/issues"
  },
  "homepage": "https://github.com/usdoj/exitscript#readme",
  "dependencies": {
    "external-link": "2.0.0",
    "simple-modal": "2.0.0"
  },
  "devDependencies": {
    "browserify": "16.0.0",
    "mkdirp": "0.5.1",
    "rimraf": "2.6.2"
  }
}
